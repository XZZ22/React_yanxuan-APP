.knowledgeHeader
  position fixed
  z-index 100
  top 0
  left 0
  width 100%
  height 45px
  background-color #FAFAFA
  display flex
  align-items center
  flex 1
  justify-content center
  .icon_left
    width 31%
    height 100%
    padding 5px
    .icon_home
      font-size 30px
  .icon_middle
    width 31%
    height 100%
    display flex
    justify-content center
    align-items center
    img
      width 66%
      height 54%
  .icon_right
    width 33%
    height 100%
    padding 5px
    .icon_search
      font-size 27px
      float right
      display inline-block
      margin-right 10px
    .icon_cart
      font-size 28px
      float right