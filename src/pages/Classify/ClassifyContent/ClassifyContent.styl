.contentContainer
  width 77%
  float left
  height 580px
  .contentWrap
    width 100%
    height: 100%;
    overflow hidden
    .contentItemHeader
      margin-top 13px
      padding 10px 8px
      display flex
      align-items center
      justify-content center
      width 100%
      height 80px
      img
        width 94%
    .contentItem
      margin-top 20px
      height 200px
      width 100%
      .contentItem_title
        padding-left 5px
        margin-left 7px
        width 90%
        height 30px
        line-height 30px
        font-size 14px
        border-bottom 1px solid #cccccc
      .contentItem_item
        height 200px
        width 90%
        margin-top 4px
        overflow hidden
        margin-left 14px
        .contentItem_item_a
          display flex
          justify-content center
          align-items center
          flex-direction column
          float left
          margin-top 6px
          text-decoration none
          padding 2px 0
          height 100px
          margin-right 3px
          margin-left 4px
          width 30%
          .contentItem_item_a_img
            width 60px
          .contentItem_item_a_p
            margin-top 4px
            text-align center
            display block
            font-size 12px
            transform scale(0.9)
            white-space nowrap
            text-overflow ellipsis
            overflow hidden
            height 20px
            color black
            width 65px