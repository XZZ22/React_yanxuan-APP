.newProductContainer
  .new_product
    width 100%
    height 100px
    background-color #F1F7FD
    text-align center
    .new_product_starting
      font-size 20px
      margin-bottom 5px
      padding-top  30px
    .new_product_alert
      text-decoration none
      span
        display inline-block
      .iconfont
        font-size 12px
